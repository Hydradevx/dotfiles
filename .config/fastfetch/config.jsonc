{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  
  "logo": {
    "type": "small",
    "source": "",
    "padding": {
      "right": 2
    },
    "printRemaining": true,
    "position": "left"
  },
  
  "display": {
    "stat": false,
    "separator": "  ",
    "brightColor": true,
    "key": {
      "width": 0
    }
  },
  
  "general": {
    "thread": true,
    "processingTimeout": 1000
  },
  
  "modules": [
